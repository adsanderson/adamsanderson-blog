		"@types/markdown-it": "12.2.3",
		"dayjs": "1.10.7",
		"eslint": "8.31.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-svelte3": "4.0.0",
		"js-yaml": "4.1.0",
		"mdsvex": "0.9.8",
